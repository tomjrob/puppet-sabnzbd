<%-categories.each do |category_hashes|
<%=	["#{category_hashes['name']}"]
<%-  category_hash_defaults = {"priority" => "0", "pp" => "0", "script" => "None", "newzbin" => '""', "dir" => '""'}
<%-  category_hashes.each do |k,v|
<%-    v == "" ? v = category_hash_defaults[k] : false
<%=    "#{k}=#{v}" %>
<%-  end -%>
<%- end if categories -%>


require 'yaml'

yaml_str = <<EOS
---
categories:
  - name: "*"
    dir: ""
    script: "None"
    priority: "0"
    pp: "3"
    newzbin: ""
  - name: "movies"
    dir: "movies"
    script: "Default"
    priority: "-100"
    pp: ""
    newzbin: ""
  - name: "tv"
    dir: "tv"
    script: "sabToSickBeard.py"
    priority: "-1"
    pp: ""
    newzbin: ""
EOS

yml = YAML.load(yaml_str)
categories = yml['categories']

@categories.each do |category_hashes|
  category_hash_defaults = {"priority" => "0", "pp" => "0", "script" => "None", "newzbin" => '""', "dir" => '""'}
  category_hashes.each_pair do |k,v|
    unless v == ""
      local_variable_set(k,v,bind)
    else
      local_variable_set(k,category_hash_defaults[k],bind)
    end
	puts priority
    puts pp
    puts script
    puts newzbin
    puts dir
  end
end if categories






